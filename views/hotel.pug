extends layout


block content
    h1= title
    -let results =[{Hotel_name: "Novotel",location: "Hyderabad",price: 130},{Hotel_name: "Oberoi",location: "Mumbai",price: 170},{Hotel_name: "JW Marriott",location: "Delhi",price: 200}]]
    div(class="container-fluid")
    div(class="row")
    -
    each h in results 
        div(class="col-sm-2") #{h.Hotel_name} #{h.location} #{h.price}